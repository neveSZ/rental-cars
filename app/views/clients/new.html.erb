<% @client.errors.full_messages.each do |message| %>
    <%= message %>
<% end %>

<%= form_with model: @client, local: true do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :cpf %>
    <%= f.text_field :cpf %>
    <%= f.label :email %>
    <%= f.text_field :email %>   
    <%= f.submit 'Enviar' %>
<% end %>